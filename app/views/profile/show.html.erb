<div class="bg-white">
  <div class="max-w-2xl mx-auto py-16 px-4 sm:py-24 sm:px-6 lg:max-w-7xl lg:px-8">
    <div class="bg-warm-gray-50">
      <div class="py-20 lg:py-28">
        <div class="relative z-10 max-w-7xl mx-auto pl-0 pr-8 sm:px-0 lg:px-4">
          <h1 class="text-4xl font-extrabold tracking-tight text-warm-gray-400 sm:text-5xl lg:text-6xl pl-0">Books you liked: </h1>
        </div>
      </div>
    </div>
    <ul>
      <% @books.each do |book| %>
        <% book.title %>
      </ul>
    <% end %>
    <div class="grid grid-cols-1 gap-y-10 sm:grid-cols-2 gap-x-6 lg:grid-cols-3 xl:grid-cols-4 xl:gap-x-8">
      <% @books.each do |book| %>
        <a class="group">
          <div class="w-full aspect-w-1 aspect-h-1 bg-gray-200 rounded-lg overflow-hidden xl:aspect-w-8 xl:aspect-h-12">
            <img src= <%= book.image_url %> alt=<%= book.title %> class="w-full h-full object-center object-cover group-hover:opacity-75">
          </div>
          <h3 class="mt-4 text-sm text-gray-700"><%= book.title %> </h3>
        </a>
      <% end %>
    </div>
    <div class="bg-warm-gray-50">
      <div class="py-16 lg:py-24">
        <div class="relative z-10 max-w-7xl mx-auto pl- pr-8 sm:px-6 lg:px-8">
          <h1 class="text-4xl font-extrabold tracking-tight text-warm-gray-900 sm:text-5xl lg:text-6xl">Books you disliked: </h1>
          <ul>
            <% @disliked.each do |dbook| %>
              <p class="mt-6 text-xl text-warm-gray-500 max-w-3xl"><%= dbook.title %></p>
            <% end %>
          </ul>
        </div>
      </div>
    </div>
  </div>
</div>
